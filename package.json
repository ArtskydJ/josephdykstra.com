{
  "name": "josephdykstra-com",
  "version": "2.2.0",
  "main": "./server.js",
  "author": {
    "name": "TehShrike",
    "email": "me@JoshDuff.com",
    "url": "http://joshduff.com"
  },
  "description": "Joseph Dykstra's site",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArtskydJ/josephdykstra-com.git"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap ./test/*.js",
    "watch": "watchify js/index.js -o js/build.js -d",
    "deploy": "nohup ploy server ./data -p 80 -a ../auth.json",
    "redeploy": "git push ploy master",
    "postinstall": "browserify js/index.js | uglifyjs -m -o js/build.js"
  },
  "devDependencies": {
    "tap": "~0.4.4"
  },
  "dependencies": {
    "level-sublevel": "~5.2.0",
    "levelup": "~0.18.2",
    "localstorage-down": "~0.4.0",
    "noddity-butler": "^1.0.0",
    "noddity-linkifier": "~2.0.0",
    "noddity-renderer": "^2.0.0",
    "pagedown": "~1.1.0",
    "ractive": "~0.5.0",
    "send": "^0.10.0"
  }
}
